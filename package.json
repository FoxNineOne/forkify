    "parcel": "^2.16.0",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "core-js": "^3.45.1",
    "fractional": "^1.0.0",
    "fracty": "^1.0.12",
    "regenerator-runtime": "^0.14.1"
  }
